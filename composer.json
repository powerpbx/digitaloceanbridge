{
    "name": "powerpbx/digitaloceanbridge",
    "description": "DigitalOcean Is A DigitalOcean Bridge For Laravel 5",
    "keywords": ["laravel", "framework", "digitalocean", "digitalocean-v2", "DigitalOceanV2", "digitalocean bridge", "bridge", "DigitalOcean", "Laravel DigitalOcean", "Laravel-DigitalOcean", "Graham Campbell", "GrahamCampbell", "PowerPBX"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        },
        {
            "name": "PowerPBX",
            "email": "devops@powerpbx.org"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/contracts": "5.5.*|5.6.*|5.7.*|5.8.*",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*",
        "graham-campbell/manager": "^4.2",
        "toin0u/digitalocean-v2": "^2.3"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^2.1",
        "graham-campbell/testbench": "^5.2",
        "guzzle/guzzle": "^3.7",
        "guzzlehttp/guzzle" : "^5.0|^6.0",
        "kriswallsmith/buzz": "^0.10",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5|^7.0|^8.0"
    },
    "suggest": {
        "kriswallsmith/buzz": "Enables the buzz connector.",
        "guzzle/guzzle": "Enables the guzzle connector.",
        "guzzlehttp/guzzle": "Enables the guzzlehttp connector."
    },
    "autoload": {
        "psr-4": {
            "PowerPBX\\DigitalOcean\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PowerPBX\\Tests\\DigitalOcean\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.2-dev"
        },
        "laravel": {
            "providers": [
                "PowerPBX\\DigitalOcean\\DigitalOceanServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
